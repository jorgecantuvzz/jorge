    "# prueba de dickey-fuller\n",
    "ADF_test1 = adfuller(pib_trim['PIB'])\n",
    "#Ho: Serie No estacionaria\n",
    "\n",
    "#significancia 5%\n",
    "alpha =0.05\n",
    "\n",
    "if ADF_test1[1] < alpha:\n",
    "    print('rechazamos Ho, La serie es estacionaria')\n",
    "else:\n",
    "    print('No rechazamos Ho, La serie es No estacionaria')"
